{
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": false,
    "source.fixAll.stylelint": false
  },
  "eslint.workingDirectories": [
  ],
  "editor.tabSize": 2,
  // 文件自动保存
  // "files.autoSave": "afterDelay",?
  // 自动格式化粘贴的代码
  "editor.formatOnPaste": true,
  "files.eol": "\n",
  "[shellscript]": {
    "files.eol": "\n"
  },
}